programa {
  funcao inicio() {
    real valorSalarioMinimo
    real salarioFuncionario
    escreva("Informe o valor do salário minímo: ")
    leia(valorSalarioMinimo)
    escreva("Informe o salário do funcionário: ")
    leia(salarioFuncionario)
    escreva("O funcionário ganha "+(valorSalarioMinimo / salarioFuncionario)+" salários mínimos")
  }
}
